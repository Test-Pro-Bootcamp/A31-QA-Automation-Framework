<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="TestNG" parallel="methods" thread-count="2">
<parameter name="BaseURL" value="https://bbb.testpro.io/"/>
    <test name="Regression Tests" preserve-order="false">
        <classes>
            <class name="LoginTests"/>
            <class name="BaseTest"/>
<!--            <class name="launchKoelTest"/>-->
            <class name="createPlaylistTest"/>
<!--            <class name="playSongTest"/>-->
            <class name="deletePlaylistTest"/>
            <class name="renamePlaylistTest"/>
            <class name="CucumberRunner"/>
            <class name="addSongToPlaylistTest"/>
        </classes>
    </test>

    <test name="Smoke Tests" preserve-order="false">
        <classes>
            <class name="launchKoelTest"/>
            <class name="LoginTests"/>
            <class name="playSongTest"/>
        </classes>
    </test>
</suite>




